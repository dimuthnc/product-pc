importPackage(org.wso2.carbon.pc.core.transfer);

var log = new Log();
if (log.isDebugEnabled()) {
    log.debug("INSIDE IMPORT PROCESS...");
}
var ip = new ProcessImport();

var server = require('store').server;
var user = server.current(session);

var files = request.getAllFiles();
var zipFile = files["processZip"];
var zipInputStream = zipFile.getStream().getStream();
var responseObj = {};
try {
    ip.importProcesses(zipInputStream, user.username);
    responseObj.error = false;
    responseObj.content = "SUCCESS";
    print(stringify(responseObj));
}catch(errorE){
    log.error(errorE);
    responseObj.error = true;
    if(errorE.message == "org.wso2.carbon.pc.core.ProcessCenterException: ALREADY_AVAILABLE"){
        responseObj.content = "ERROR: Tried to Add Already Available Process";
        print(stringify(responseObj));
    }else {
        responseObj.content = "FAIL";
        print(stringify(responseObj));
    }
}
