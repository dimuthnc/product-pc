<script type="text/javascript">
window.onload = function() {
   $('.selectpicker').selectpicker({
        style: 'btn',
        size: 4
    });
    loadTaskList('taskInstanceIdExecTimeTaskList', 'taskInstanceIdExecTimeBarChart', drawExecutionTimeVsTaskInstanceIdResult);
};
</script>

<div style="padding-top: 10px;padding-left: 10px;padding-right: 10px;">
   <div class="form-inline" role="form">
      <div class="form-group">
        <select id="taskInstanceIdExecTimeTaskList" class="selectpicker form-control"></select>
      </div>
      <div class="input-group form-group">
         <label  class="input-group-addon" for="taskInstanceIdExecTimeStartDate">Start Date</label>
         <input id="taskInstanceIdExecTimeStartDate" name="taskInstanceIdExecTimeStartDate" type="text" class="form-control" onfocus="setDatePicker('taskInstanceIdExecTimeStartDate')" placeholder="MM/DD/YYYY"/>
      </div>
      <div class="input-group form-group">
         <label class="input-group-addon" for="taskInstanceIdExecTimeEndDate">End Date</label>
         <input id="taskInstanceIdExecTimeEndDate" name="taskInstanceIdExecTimeEndDate" type="text" class="form-control" onfocus="setDatePicker('taskInstanceIdExecTimeEndDate')" placeholder="MM/DD/YYYY"/>
      </div>
      <div class="form-group">
         <select id="taskInstanceIdExecTimeOrder" class="selectpicker form-control">
            <option value="top">Top</option>
            <option value="bottom">Bottom</option>
         </select>
      </div>
      <div class="form-group">
         <select id="taskInstanceIdExecTimeLimit" class="selectpicker form-control" onchange="selectPickerValChange(this)">
            <option value="other">Set a new value</option>
            <option selected="selected" value="3">3</option>
            <option value="5">5</option>
            <option value="10">10</option>
            <option value="50">50</option>
            <option value="100">100</option>
         </select>
      </div>
      <button class="btn btn-default btn-info" onclick="drawExecutionTimeVsTaskInstanceIdResult('taskInstanceIdExecTimeBarChart', this)">Apply Filter</button>
   </div>
</div>
<div class="mainGraph" id="taskInstanceIdExecTimeBarChart" style="width: 70%;height: 80%;"></div>